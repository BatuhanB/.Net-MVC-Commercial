
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<script src="https://code.jquery.com/jquery-3.6.0.js" integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk=" crossorigin="anonymous"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<div class="container-fluid">
    <h2>Alert Sayfasi</h2>
    <button class="btn btn-primary" id="btn1">Buton1</button>
    <button class="btn btn-warning" id="btn2">Buton2</button>
    <button class="btn btn-info" id="btn3">Buton3</button>
    <button class="btn btn-danger" id="btn4">Buton4</button>
    <button class="btn btn-dark" id="btn5">Buton5</button>
    <script>
        $('#btn1').click(function () {
            swal("Kategori başarılı bir şekilde eklendi!")
        });
        $('#btn2').click(function () {
            swal("Kategori Ekleme", "Kategori başarılı bir şekilde eklendi!")
        });
        $('#btn3').click(function () {
            swal("Kategori Ekleme", "Kategori başarılı bir şekilde eklendi!", "success")
        });
        $('#btn4').click(function () {
            swal("Click on either the button or outside the modal.")
                .then((value) => {
                    swal(`The returned value is: ${value}`);
                });
        });
        $('#btn5').click(function () {
            swal({
                title: "Are you sure?",
                text: "Once deleted, you will not be able to recover this imaginary file!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        swal("Poof! Your imaginary file has been deleted!", {
                            icon: "success",
                        });
                    } else {
                        swal("Your imaginary file is safe!");
                    }
                });
        });
    </script>
</div>
